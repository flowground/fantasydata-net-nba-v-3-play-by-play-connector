    "envVars": {},
    "credentials": {
        "fields": {
            "server": {
                "label": "Server",
                "viewClass": "SelectView",
                "model": [
                    "http://api.fantasydata.net/v3/nba/pbp",
                    "https://api.fantasydata.net/v3/nba/pbp",
                    "--- Custom URL"
                ],
                "required": true
            },
            "otherServer": {
                "label": "Custom Server URL",
                "viewClass": "TextFieldView"
            },
            "apiKeyHeader": {
                "label": "Ocp-Apim-Subscription-Key (apiKeyHeader)",
                "viewClass": "TextFieldView"
            },
            "apiKeyQuery": {
                "label": "subscription-key (apiKeyQuery)",
                "viewClass": "TextFieldView"
            }
        }
    },
    "triggers": {
